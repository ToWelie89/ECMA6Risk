{
  "name": "TotalRisk",
  "productName": "TotalRisk",
  "version": "1.2.16",
  "description": "A web application game of the classic board game Risk",
  "main": "electron/main.js",
  "scripts": {
    "start": "http-server",
    "build-all": "npm run build-32-bit && npm run build-64-bit",
    "build-32-bit": "npm run elec-package-win-32 && npm run elec-installer-win-32",
    "build-64-bit": "npm run elec-package-win-64 && npm run elec-installer-win-64",
    "elec-package-win-32": "electron-packager . TotalRisk --platform win32 --arch ia32 --out electron/dist/ --overwrite --icon electron/assets/icon.ico",
    "elec-package-win-64": "electron-packager . TotalRisk --platform win32 --arch x64 --out electron/dist/ --overwrite --icon electron/assets/icon.ico",
    "elec-installer-win-32": "electron-installer-windows --src electron/dist/TotalRisk-win32-ia32/ --dest electron/dist/installers/windows32/ --config electron/config-windows.json",
    "elec-installer-win-64": "electron-installer-windows --src electron/dist/TotalRisk-win32-x64/ --dest electron/dist/installers/windows64/ --config electron/config-windows.json"
  },
  "keywords": [
    "angular",
    "babel",
    "karma",
    "jasmine",
    "workflow",
    "electron",
    "ecma6",
    "es2015",
    "less",
    "grunt",
    "risk",
    "game"
  ],
  "author": "Martin Sonesson <sonesson8909@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ToWelie89/ECMA6Risk/issues",
    "email": "sonesson8909@hotmail.com"
  },
  "homepage": "https://github.com/ToWelie89/ECMA6Risk.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/ToWelie89/ECMA6Risk.git"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-sanitize": "^1.6.6",
    "angular-ui-bootstrap": "^2.5.3",
    "angularjs-slider": "^5.8.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.10.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.6",
    "esprima": "^4.0.0",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.2.0",
    "grunt-bump": "^0.8.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-less": "^1.4.1",
    "grunt-contrib-uglify": "^3.1.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-karma": "^2.0.0",
    "grunt-notify": "^0.4.5",
    "grunt-replace": "^1.0.1",
    "http-server": "^0.9.0",
    "jquery": "^2.2.4"
  },
  "devDependencies": {
    "electron-installer-windows": "^1.0.2",
    "electron-package": "^0.1.0",
    "grunt-cli": "^1.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-babel-polyfill": "0.0.5",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31"
  }
}
